    "## Define a class\n",
    "\n",
    "class PythonSwitch:\n",
    "    def switch(self, dayOfWeek):\n",
    "        return getattr(self, 'case_'+ str(dayOfWeek), lambda:'wrong day of a week')()\n",
    "    def case_1(self):\n",
    "        return 'Monday'\n",
    "    def case_2(self):\n",
    "        return 'Tuesday'\n",
    "    def case_3(self):\n",
    "        return 'Wednesday'\n",
    "    \n",
    "    ## driver a code\n",
    "    \n",
    "p = PythonSwitch()\n",
    "print(p.switch(1))\n",
    "print(p.switch(0))\n",
